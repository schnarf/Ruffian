// Prototypes for standard library functions
def printInt(int n);
def printFloat(float f);
def printDouble(double f);
def printNewline();
def rand() -> int;

def print_array(int n, int[] a) {
	foreach (int i; 0:n) {
		printInt( a[i] );
	}
}

def insertion_sort(int n, int[] a ) {
	foreach (int i; 1:n) {
		int key= a[i];
		int j= i - 1;
		while( j >= 0 & a[j] > key ) {
			a[j+1]= a[j];
			j= j - 1;
		}
		a[j+1]= key;
	}
}

